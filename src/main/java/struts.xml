<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"  
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
	<constant name="struts.enable.DynamicMethodInvocation"
		value="false" />
	<constant name="struts.devMode" value="true"></constant>

	<package name="MyPackage" namespace="/"
		extends="struts-default">
		<action name="goToCodeOnLine"
			class="cn.sjy.action.GoToCodeOnLineAction">
			<result name="success">./pages/codeOnLine.jsp</result>
		</action>
		<action name="goToSignIn"
			class="cn.sjy.action.GoToSignInAction">
			<result name="success">./pages/signIn.jsp</result>
		</action>
	</package>
</struts>